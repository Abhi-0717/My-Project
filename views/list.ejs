	<body>
        <table style="width: 90%; margin:50px 0px 50px 80px; border: 1px solid black; ">
            <tr border="1">
                <th>Name</th>
                <th>Email</th>
                <th>Phone</th>
                <th> Action </th>
            </tr>
            <% result.forEach(function(item){ %>
            <tr border="1">
                <td><%= item.name %></td>
                <td><%= item.email %></td>
                <td><%= item.phone %></td>
                <td><a href="/result/update<% item._id %>">Edit</a>
                    <a href="delete<% item._id %>">Delete</a>
                </td>
            </tr>
            <% }) %> 
        </table>
    </body>
